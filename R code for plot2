data=read.table("household_power_consumption.txt",header=TRUE, sep=";",na.strings = "?",stringsAsFactors = FALSE)
head(data)
summary(data)
mydata=subset(data,data$Date=="1/2/2007" | data$Date=="2/2/2007")
mydata$Time=strptime(mydata$Time,format="%H:%M:%S")
mydata$Date=as.Date(mydata$Date, format= "%d/%m/%Y")
plot(mydata$Time,as.character(as.numeric(mydata$Global_active_power)), type = "l",ylab = "Global Active Power")
dev.copy(png,filename="plot2.png")
dev.off()
